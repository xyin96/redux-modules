{
  "env": {
    "browser": true
  },
  "plugins": [
    "flowtype"
  ],
  "rules": {
    "import/prefer-default-export": "off",
    "import/no-duplicates": "error",
    "camelcase": "off",
    "no-duplicate-imports": "off",
    "no-unused-vars": "error",
    "no-underscore-dangle": "off",
    "no-shadow": "off",
    "global-require": "off",
  },
  "settings": {
    "import/resolver": "webpack"
  }
}
